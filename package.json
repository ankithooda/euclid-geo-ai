{
  "name": "euclid-geo-ai",
  "version": "1.0.0",
  "description": "Interactive 2D geometry application using JSXGraph",
  "main": "dist/index.html",
  "scripts": {
    "dev": "http-server dist -p 8000 -c-1",
    "build": "esbuild src/app.js --bundle --outfile=dist/app.js && cp src/index.html dist/index.html",
    "watch": "esbuild src/app.js --bundle --outfile=dist/app.js --watch",
    "serve": "npm run build && npm run dev"
  },
  "keywords": ["geometry", "jsxgraph", "interactive", "2d"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.19.0",
    "http-server": "^14.1.1"
  }
}
